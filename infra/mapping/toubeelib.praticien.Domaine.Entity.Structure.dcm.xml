<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="toubeelib\praticien\Domaine\Entity\Structure" table="structure">
        <id name="id" type="string" column="id">
            <generator strategy="NONE"/>
        </id>
        <field name="nom" type="string" length="48"/>
        <field name="adresse" type="text"/>
        <field name="ville" type="string" length="128" nullable="true"/>
        <field name="code_postal" type="string" length="12" nullable="true"/>
        <field name="telephone" type="string" length="24" nullable="true"/>

        <one-to-many field="praticiens" target-entity="toubeelib\praticien\Domaine\Entity\Praticien" mapped-by="structure"/>
    </entity>

</doctrine-mapping>