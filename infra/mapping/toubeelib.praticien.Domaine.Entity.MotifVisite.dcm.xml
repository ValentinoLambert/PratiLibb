<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="toubeelib\praticien\Domaine\Entity\MotifVisite" table="motif_visite">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="libelle" type="string" length="128"/>

        <many-to-one field="specialite" target-entity="toubeelib\praticien\Domaine\Entity\Specialite" inversed-by="motifs">
            <join-column name="specialite_id" referenced-column-name="id"/>
        </many-to-one>
    </entity>

</doctrine-mapping>
